{
    "[haskell]": {
        "editor.tabSize": 2
    },
    "[html]": {
        "editor.tabSize": 2
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "chat.commandCenter.enabled": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.bracketPairColorization.enabled": false,
    "editor.fontFamily": "'Fira Code'",
    "editor.fontLigatures": true,
    "editor.fontSize": 18,
    "editor.formatOnSave": true,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.lightbulb.enabled": "off",
    "editor.lineHeight": 30,
    "editor.occurrencesHighlight": "off",
    "editor.renderLineHighlight": "none",
    "editor.roundedSelection": false,
    "editor.smoothScrolling": true,
    "editor.stickyTabStops": true,
    "editor.wordWrap": "on",
    "errorLens.onSave": true,
    "explorer.confirmDelete": false,
    "extensions.trustedPublishers": [
        "llvm-vs-code-extensions",
        "vadimcn",
        "vscjava",
        "usernamehw",
        "tamasfe",
        "tombonnike",
        "haskell",
        "justusadam",
        "ecmel",
        "redhat",
        "james-yu",
        "ms-python",
        "rust-lang"
    ],
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "formattingToggle.affects": [
        "editor.formatOnSave"
    ],
    "git.confirmSync": false,
    "haskell.manageHLS": "GHCup",
    "java.project.sourcePaths": [
        "src"
    ],
    "latex-workshop.formatting.latex": "latexindent",
    "latex-workshop.latex.autoClean.run": "onBuilt",
    "latex-workshop.latex.clean.fileTypes": [
        "%DOCFILE%.aux",
        "%DOCFILE%.bbl",
        "%DOCFILE%.blg",
        "%DOCFILE%.idx",
        "%DOCFILE%.ind",
        "%DOCFILE%.lof",
        "%DOCFILE%.lot",
        "%DOCFILE%.out",
        "%DOCFILE%.toc",
        "%DOCFILE%.acn",
        "%DOCFILE%.acr",
        "%DOCFILE%.alg",
        "%DOCFILE%.glg",
        "%DOCFILE%.glo",
        "%DOCFILE%.gls",
        "%DOCFILE%.fls",
        "%DOCFILE%.log",
        "%DOCFILE%.fdb_latexmk",
        "%DOCFILE%.snm",
        "%DOCFILE%.synctex(busy)",
        "%DOCFILE%.synctex.gz(busy)",
        "%DOCFILE%.nav",
        "%DOCFILE%.vrb",
        "%DOCFILE%.xdv",
        "%DOCFILE%.synctex.gz",
        "%DOCFILE%.run.xml",
        "%DOCFILE%.bcf"
    ],
    "latex-workshop.latex.clean.method": "glob",
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk (xelatex)",
            "tools": [
                "xelatexmk"
            ]
        }
    ],
    "latex-workshop.message.error.show": false,
    "latex-workshop.message.warning.show": false,
    "redhat.telemetry.enabled": false,
    "terminal.integrated.focusAfterRun": "terminal",
    "terminal.integrated.fontLigatures.enabled": true,
    "terminal.integrated.fontSize": 18,
    "terminal.integrated.smoothScrolling": true,
    "window.autoDetectColorScheme": true,
    "window.commandCenter": false,
    "window.menuBarVisibility": "compact",
    "window.restoreWindows": "none",
    "workbench.editor.empty.hint": "hidden",
    "workbench.editor.tabActionCloseVisibility": false,
    "workbench.layoutControl.enabled": false,
    "workbench.panel.opensMaximized": "always",
    "workbench.preferredDarkColorTheme": "Monochromator Dark",
    "workbench.preferredLightColorTheme": "Monochromator Light",
    "workbench.startupEditor": "none"
}

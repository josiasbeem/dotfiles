{
    "[haskell]": {
        "editor.tabSize": 2
    },
    "[html]": {
        "editor.tabSize": 2
    },
    "[racket]": {
        "editor.tabSize": 2
    },
    "breadcrumbs.enabled": false,
    "chat.commandCenter.enabled": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.cursorBlinking": "solid",
    "editor.fontFamily": "Adwaita Mono",
    "editor.fontSize": 18,
    "editor.formatOnSave": true,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.lightbulb.enabled": "off",
    "editor.lineHeight": 1.5,
    "editor.stickyTabStops": true,
    "editor.wordWrap": "on",
    "explorer.confirmDelete": false,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "formattingToggle.affects": [
        "editor.formatOnSave"
    ],
    "git.autofetch": true,
    "git.confirmSync": false,
    "latex-workshop.formatting.latex": "latexindent",
    "latex-workshop.latex.clean.method": "glob",
    "latex-workshop.latex.recipes": [
        {
            "name": "pdflatex-recipe",
            "tools": [
                "compile",
                "compile",
                "clean"
            ]
        }
    ],
    "latex-workshop.latex.tools": [
        {
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-shell-escape",
                "%DOC%"
            ],
            "command": "pdflatex",
            "name": "compile"
        },
        {
            "args": [
                "-f",
                "%DOCFILE%.aux",
                "%DOCFILE%.log",
                "%DOCFILE%.synctex.gz",
                "%DOCFILE%.thm",
                "%DOCFILE%.toc"
            ],
            "command": "rm",
            "name": "clean"
        }
    ],
    "latex-workshop.message.error.show": false,
    "latex-workshop.message.warning.show": false,
    "latex-workshop.view.pdf.invert": 0.882,
    "latex-workshop.view.pdf.invertMode.enabled": "auto",
    "material-icon-theme.saturation": 0,
    "redhat.telemetry.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "sqltools.highlightQuery": false,
    "terminal.integrated.focusAfterRun": "terminal",
    "terminal.integrated.fontSize": 18,
    "window.autoDetectColorScheme": true,
    "window.restoreWindows": "none",
    "workbench.editor.tabActionCloseVisibility": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.layoutControl.enabled": false,
    "workbench.panel.opensMaximized": "always",
    "workbench.preferredDarkColorTheme": "Monochromator Dark Emerald",
    "workbench.preferredLightColorTheme": "Monochromator Light Amber",
    "workbench.settings.editor": "json",
    "workbench.startupEditor": "none"
}
